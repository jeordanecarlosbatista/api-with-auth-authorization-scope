{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off",
        "import/export": "off",
        "import/extensions": ["error", "never", {"ignorePackages": true } ],
        "max-len": ["error", { "code": 500 }]
    }
}
